---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>draftcode.github.com</title>
    <link href="http://draftcode.github.com/atom.xml" rel="self" />
    <link href="http://draftcode.github.com/" />
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://draftcode.github.com/</id>
    <author>
        <name>draftcode</name>
        <email>draftcode@gmail.com</email>
    </author>
    {% for post in site.posts limit:10 %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://draftcode.github.com{{ post.url }}" />
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://draftcode.github.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
        <author>
            <name>draftcode</name>
            <email>draftcode@gmail.com</email>
        </author>
    </entry>
    {% endfor %}
</feed>
